{
  "name": "ant-frontend-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "4.7.0",
    "@metamask/detect-provider": "1.2.0",
    "@metamask/providers": "^9.0.0",
    "antd": "4.21.0",
    "axios": "0.25.0",
    "classnames": "2.3.1",
    "date-fns": "2.28.0",
    "eth-rpc-errors": "4.0.3",
    "ethers": "5.6.8",
    "history": "5.2.0",
    "node": "^17.7.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.0.1",
    "react-router-dom": "6.3.0",
    "react-scripts": "5.0.1",
    "react-toastify": "9.0.4",
    "reactotron-react-js": "3.3.7",
    "reactotron-redux": "3.1.3",
    "redux": "4.1.2",
    "redux-first-history": "5.0.8",
    "redux-persist": "6.0.0",
    "redux-saga": "1.1.3",
    "sass": "1.52.3",
    "sass-loader": "13.0.0",
    "typescript": "4.6.4",
    "web3": "1.7.3",
    "web3-eth-contract": "1.7.3",
    "web3-utils": "1.7.3"
  },
  "devDependencies": {
    "@craco/craco": "6.4.3",
    "@storybook/addon-actions": "6.4.13",
    "@storybook/addon-essentials": "6.4.13",
    "@storybook/addon-links": "6.4.13",
    "@storybook/builder-webpack5": "6.4.13",
    "@storybook/manager-webpack5": "6.4.13",
    "@storybook/node-logger": "6.4.13",
    "@storybook/preset-create-react-app": "4.0.0",
    "@storybook/react": "6.4.13",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.0.0",
    "@testing-library/user-event": "13.2.1",
    "@types/classnames": "2.3.1",
    "@types/history": "4.7.8",
    "@types/jest": "27.0.1",
    "@types/node": "16.7.13",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.3",
    "@types/react-redux": "7.1.24",
    "@types/redux-saga": "0.10.5",
    "@typescript-eslint/eslint-plugin": "5.26.0",
    "@typescript-eslint/parser": "5.26.0",
    "awesome-typescript-loader": "5.2.1",
    "browser-sync": "2.27.9",
    "craco-less": "2.0.0",
    "cross-env": "7.0.3",
    "css-loader": "6.7.1",
    "env-cmd": "10.1.0",
    "eslint": "8.16.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.5.0",
    "husky": "7.0.4",
    "plop": "3.0.5",
    "webpack": "5.72.0"
  },
  "scripts": {
    "start": "env-cmd -f .env.development craco start",
    "build:dev": "env-cmd -f .env.development craco build",
    "build:prod": "env-cmd -f .env.production craco build",
    "build:stage": "env-cmd -f .env.stage craco build",
    "test": "craco test",
    "lint-es": "eslint . --cache --fix --ext js,jsx,ts,tsx",
    "lint-ts": "tsc --noEmit",
    "pre-commit": "yarn lint-es && yarn lint-ts",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "new": "plop --plopfile generators/index.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
